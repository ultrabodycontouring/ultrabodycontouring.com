@{
	var SiteName = "Ultra Body Contouring";
	  
	if (ViewBag.Title == "")
	{
		ViewBag.Title = SiteName;
	}
	else
	{
		ViewBag.Title = ViewBag.Title + " - " + SiteName;
	}			
}

<!DOCTYPE html>

<html class="no-js">

	<head>

		<!-- Metadata -->
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta name="description" content="">
		<meta name="keywords" content="">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>@ViewBag.Title</title>

		<!-- Sitemap, Favicon, and Apple Touch Icon -->
		<link rel="sitemap" type="application/xml" title="Sitemap" href="/sitemap.xml" />
		<link rel="icon" type="image/x-icon" href="/favicon.png" />
		<link rel="shortcut icon" type="image/x-icon" href="/favicon.png" />
		<link rel="apple-touch-icon" href="/apple-touch-icon.png" />

		<!-- Stylesheets -->
		<link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:400,100,300|Open+Sans'>
		<link rel="stylesheet" href="/Content/Styles/style.css">
		<link rel="stylesheet" href="/Content/Styles/Templates/template.css">
		@RenderSection("Styles", false)

		<!-- Startup Scripts -->
		@RenderSection("Startup-Scripts", false)

    </head>

    <body>
	
		<div id="wrapper">

			<header id="header" role="banner">

				@Html.Partial("_Header")

			</header>

			<nav id="nav" role="navigation">

				@Html.Partial("_Navigation")

			</nav>

			<article id="content" role="main">

				@RenderBody()

			</article>

			<footer id="footer" role="contentinfo">

				@Html.Partial("_Footer")
			
			</footer>

		</div>

		<!-- jQuery -->
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="/Content/Scripts/jquery-1.10.2.min.js"><\/script>')</script>

		<!-- Scripts -->
		@RenderSection("Scripts", false)
		
        @Html.Partial("_Analytics")

    </body>

</html>
